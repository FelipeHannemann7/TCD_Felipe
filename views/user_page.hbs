<div class="container marketing">

  <div class="row">
    <div class="col-lg-12">
      {{> user_avatar}}
      <p></p>
      <div class="form-group row">
        <label for="colFormLabelLg" class="col-sm-1 col-form-label col-form-label-lg">Nome:</label>
        <div class="col-sm-10">
          <input readonly='true' type="text" value={{user.name}} class="form-control form-control-lg"
            id="colFormLabelLg" placeholder="col-form-label-lg">
        </div>
      </div>
      <div class="form-group row">
        <label for="colFormLabelLg" class="col-sm-1 col-form-label col-form-label-lg">Email:</label>
        <div class="col-sm-10">
          <input readonly='true' type="text" value={{user.username}} class="form-control form-control-lg"
            id="colFormLabelLg" placeholder="col-form-label-lg">
        </div>
      </div>
      <div class="form-group row">
        <label style="text-align: right;" for="colFormLabelLg" class="col-sm-6 col-form-label col-form-label-lg">Data Nascimento:</label>
        <div class="col-sm-5">
          <input readonly='true' type="text" value={{user.dtNascimento}} class="form-control form-control-lg"
            id="colFormLabelLg" placeholder="col-form-label-lg">
        </div>
      </div>



    </div>


  </div>